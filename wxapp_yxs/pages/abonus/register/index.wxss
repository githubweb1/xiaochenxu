/* pages/abonus/index/index.wxss */
page {
  background: #f5f5f5;
}
.xj-info-header {
  padding: 15rpx 0;
  font-size: 30rpx;
  color: #333
}
.xj-info-header text {
  color: #f55;
}
.oy_form_pointer {
  color: #f30;
  margin-top: 13rpx;
  margin-right: 5rpx;
}
.oy_form_title {
  width: 150rpx;
}
.oy_form_list {
  padding: 20rpx 0;
  font-size: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.oy_form_right {

}
.xj-btn button {
  margin-top: 20rpx;
  background: #f55;
  width: 94%;
  color: white;
  font-size: 30rpx;
}
.xj-top {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
}
.xj-top image {
  width: 30rpx;
  height: 30rpx;
}
.xj-bottom {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.xj-bottom image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.xj-bottom-become {
  

}
.fui-checkbox-primary {
  font-size: 30rpx;
  padding: 20rpx 20rpx;
  color: #666;
  margin-top: 20rpx;
  padding-bottom:40rpx;
}
.xj-content{
 display:flex;
flex-direction:column;
justify-content:center;
align-items:center;
margin-top:60rpx;

}
.xj-content image{
  width:250rpx;
  height: 250rpx;
}
.xj-content view{
    margin-top:60rpx;
    font-size:30rpx;
}
.xj-content navigator{
margin-top:40rpx;
height:88rpx;
color:white;
background:#ff5555;
text-align:center;
width:94%;
line-height:88rpx;
border-radius:15rpx;
}
/* 协议 */
.view-eno-bg {
  background: #000;
  opacity: 0.5;
  width: 100%;
  height: 100%;
  z-index: 1000;
  position: fixed;
  top: 0;
  left: 0;
}
.read{
  background:#fff;
  display:flex;
  font-size:15px;
  width:80%;
  z-index:10001;
  position:fixed;
  height:500rpx;
  top:25%;
  left:10%;
  flex-direction:column;
  align-items:center;
  border-radius: 20rpx;
}
.btn{
  width:50%;
  display:flex;
  justify-content:center;
  align-items:center;
  position:absolute;
  bottom:20rpx;
  background:#ff5555;
  color:white;
  padding:20rpx;
  border-radius:35rpx;
}
.bg-title{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 36rpx;
  color: #333;
}
.bg-content{
  width: 90%;
  height: 65%;
  margin:0 auto;
  font-size: 30rpx;
  color: #666;
  overflow: scroll;
  display: flex;
  flex-direction: column;
  text-indent:2em;
}