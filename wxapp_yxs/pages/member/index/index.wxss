
/*service button*/
.quic{
  z-index:999;
  width:130px;
}
 .quic .info{
  position:fixed;
  z-index: 19;
  bottom:100rpx;
  right:30rpx;
  padding:20rpx;
 }
.quicbg button{
  background-color: rgba(0, 0, 0, 0);
  color:white;
  display:flex;
  flex-direction:row;
  padding:0px;

}
.quicbg button::after{ border:none !important;}
.quicbg button .pic{
  width:45rpx;
  height:45rpx;
  line-height:45rpx;
  background:#000;
  opacity: 0.7;
  font-size:22rpx;
  border-radius:100%;
  padding:22rpx;
}
.quicbg button .pic image{
  width:45rpx;
  height:45rpx;
  vertical-align:sub;
}


/* pages/center/index.wxss */
.oy_center_head{
