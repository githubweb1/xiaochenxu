page{
	word-break: break-all;
	font-size:28rpx;
  	background: #fff;
}

.oy_relative_box{
    position: relative;

}
.oy_badge{
   position: absolute;
    font-size: 44rpx;
    height: 64rpx;
    top:-32rpx;
    left:50%;
    margin-left:-16rpx;
    color:#fff;
    padding:0 20rpx;
    background-color: #F00;
    border-radius: 32rpx;
    word-break: keep-all;
    transform: scale(0.5);
   -webkit-transform: scale(0.5);
}
.oy_badge1{
    position: absolute;
    font-size: 44rpx;
    height: 64rpx;
    width:64rpx;
    top:-32rpx;
    left:50%;
    color:#fff;
    border-radius: 32rpx;
    background-color: #F00;
    word-break: keep-all;
    transform: scale(0.5);
   -webkit-transform: scale(0.5);
}
/* 底部tabbar */

.view-tabbar {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  background: #fafafa;
  padding:10rpx 0;
}

.view-tabbar-content {
  width: 20%;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.view-tabbar-image {
  width: 50rpx !important;
  height: 50rpx !important;
}

.view-tabbar-text {
  font-size: 25rpx;
  color: #999;
}

.oy-ef-line{
	border-bottom:10rpx solid #eff1f6;

}
